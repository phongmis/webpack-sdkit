{
  "name": "webpack-devserver",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "serve -d dist",
    "startdefault": "webpack serve --mode development --port 3000 --host 0.0.0.0",
    "builddefault": "webpack --progress",
    "build-mix": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lx-watch": "concurrently  \"serve -d public-mix\" \"mix watch\"",
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "webpack --progress --config config/webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.6.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jquery": "^3.6.0",
    "laravel-mix": "^6.0.16",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.4.0",
    "path": "^0.12.7",
    "postcss": "^8.1.2",
    "postcss-loader": "^5.2.0",
    "serve": "^13.0.2",
    "style-loader": "^2.0.0",
    "vue": "^2.6.12",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "concurrently": "^6.0.2",
    "state-machine": "^1.0.4"
  }
}
